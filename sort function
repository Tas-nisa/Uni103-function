#include<stdio.h>
void sort(int a[],int len){
    int i,j,temp;
    //bubble algorithm for sorting
    for(i=0;i<len-2;i++){
        for(j=i+1;j<len-1;j++){
            if(a[i]>a[j]){
                temp=a[i];
                a[i]=a[j];
                a[j]=temp;
            }
        }
    }

}
int main(){
    int arr[5],i;
    printf("Enter 5 number");
    for(i=0;i<5;i++)
        scanf("%d",&arr[i]);
        
    sort(arr,5);
    
    printf("After sorting array :");
    for(i=0;i<5;i++)
        printf("%d\n",arr[i]);

}
