#include<stdio.h>
int is_uppercase(char ch){
    if(ch>='A'&&ch<='Z')
        return 1;
    else
        return 0;

}

int main(){
    char ch;
    int result;
    printf("Enter a character : ");
    scanf(" %c",&ch);
    result=is_uppercase(ch);
    if(result==1)
        printf("You have entered uppercase");
    else
        printf("You have not entered uppercase ");
}
